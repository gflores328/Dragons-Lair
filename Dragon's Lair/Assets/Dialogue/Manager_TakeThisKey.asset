  m_Script: {fileID: 11500000, guid: 637b94a3e7538e246aca53b4a6060c69, type: 3}
  m_Name: Manager_TakeThisKey
  m_EditorClassIdentifier: 
  dialogueArray:
  - dialogue: The basement door's locked; where can I find the key?
    name: Owen Stone
    bust: {fileID: 21300000, guid: df9dfba447d21d74c9d483b02f3f8b20, type: 3}
    bustSide: 0
    audioClip: {fileID: 8300000, guid: 5c5fd84c7f2bce040b88e0385745974f, type: 3}
  - dialogue: The basement? Why do you need to go in there?
    name: Manager
    bust: {fileID: 21300000, guid: 7f010a19b347b404cb8c0bfae0f3a6a2, type: 3}
    bustSide: 1
    audioClip: {fileID: 8300000, guid: 362423a47d0f1f94cbd766822b7a051c, type: 3}
  - dialogue: Your little informant told me that's the last known location of one
      of those kids.
    name: Owen Stone
    bust: {fileID: 21300000, guid: df9dfba447d21d74c9d483b02f3f8b20, type: 3}
    bustSide: 0
    audioClip: {fileID: 8300000, guid: 2ceab752564b78c4daccd671af0b4bf2, type: 3}
  - dialogue: I see...
    name: Manager
    bust: {fileID: 21300000, guid: 7f010a19b347b404cb8c0bfae0f3a6a2, type: 3}
    bustSide: 1
    audioClip: {fileID: 8300000, guid: 13e2947f97154c84085e6787709b6f32, type: 3}
  - dialogue: I keep it locked up to stop the delinquents from messing with the machines.
    name: Manager
    bust: {fileID: 21300000, guid: 7f010a19b347b404cb8c0bfae0f3a6a2, type: 3}
    bustSide: 1
    audioClip: {fileID: 8300000, guid: 3dfffad37f6e7cd4b8e19e36ceb5757d, type: 3}
  - dialogue: The VR machine in there took quite the beating, so don't go messing
      with it.
    name: Manager
    bust: {fileID: 21300000, guid: 7f010a19b347b404cb8c0bfae0f3a6a2, type: 3}
    bustSide: 1
    audioClip: {fileID: 8300000, guid: 241224c696f55dc49a3b8769e5135e1f, type: 3}
  - dialogue: But, if you really need to go down there, then, take this key.
    name: Manager
    bust: {fileID: 21300000, guid: 7f010a19b347b404cb8c0bfae0f3a6a2, type: 3}
    bustSide: 1
    audioClip: {fileID: 8300000, guid: 02d902d8ce3b2524e8187189548716ba, type: 3}
  - dialogue: +1 Key
    name: 
    bust: {fileID: 0}
    bustSide: 2
    audioClip: {fileID: 8300000, guid: 4c51f8c3f7cc6d64cad34e02261db095, type: 3}
  - dialogue: Good luck down there! You'll need it...
    name: Manager
    bust: {fileID: 21300000, guid: 7f010a19b347b404cb8c0bfae0f3a6a2, type: 3}
    bustSide: 1
    audioClip: {fileID: 8300000, guid: d92c1ee6e5d29c64384040dc197c28c4, type: 3}
