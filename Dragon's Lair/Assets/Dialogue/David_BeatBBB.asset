    bust: {fileID: 21300000, guid: df9dfba447d21d74c9d483b02f3f8b20, type: 3}
    bustSide: 0
    audioClip: {fileID: 8300000, guid: 643c38684db4b3d4abb195f702ca5076, type: 3}
