    bust: {fileID: 21300000, guid: df9dfba447d21d74c9d483b02f3f8b20, type: 3}
    bustSide: 0
    audioClip: {fileID: 8300000, guid: 783bd0ce031f1324eb3a376f77f0f090, type: 3}
