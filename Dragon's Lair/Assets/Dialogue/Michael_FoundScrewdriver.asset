  dialogueArray:
  - dialogue: Found the screwdriver.
    name: Owen Stone
    bust: {fileID: 21300000, guid: df9dfba447d21d74c9d483b02f3f8b20, type: 3}
    bustSide: 0
    audioClip: {fileID: 8300000, guid: 01c9662bb4c6cd04ea02a6879714e64b, type: 3}
  - dialogue: Great! Now I can work on this!
    name: Michael
    bust: {fileID: 21300000, guid: a6fff8e7d8a60f047a66495dc36bc458, type: 3}
    bustSide: 1
    audioClip: {fileID: 8300000, guid: baf126ab315126541ad53f4c23ddfe3c, type: 3}
  - dialogue: I assume I'm going to need another coin for this machine? So hand it
