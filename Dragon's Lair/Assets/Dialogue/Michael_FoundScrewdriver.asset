    bust: {fileID: 21300000, guid: a6fff8e7d8a60f047a66495dc36bc458, type: 3}
    bustSide: 1
    audioClip: {fileID: 8300000, guid: 78a7a75c717961a49a19790750cb373d, type: 3}
  - dialogue: Where can I find her?
    name: Owen Stone
    bust: {fileID: 21300000, guid: df9dfba447d21d74c9d483b02f3f8b20, type: 3}
    bustSide: 0
    audioClip: {fileID: 8300000, guid: 36305fe9474259548ae55c80ffdcfcce, type: 3}
  - dialogue: She should be upstairs in the green hoodie.
    name: Michael
