MonoBehaviour:
  m_ObjectHideFlags: 0
  m_CorrespondingSourceObject: {fileID: 0}
  m_PrefabInstance: {fileID: 0}
  m_PrefabAsset: {fileID: 0}
  m_GameObject: {fileID: 0}
  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: 637b94a3e7538e246aca53b4a6060c69, type: 3}
  m_Name: Michael_AskFix
  m_EditorClassIdentifier: 
  dialogueArray:
  - dialogue: Hey kid, I heard you were good at fixing things.
    name: Owen Stone
    bust: {fileID: 21300000, guid: df9dfba447d21d74c9d483b02f3f8b20, type: 3}
    bustSide: 0
    audioClip: {fileID: 0}
  - dialogue: I, uh, yes? Who are you?
    name: Michael
    bust: {fileID: 0}
    bustSide: 1
    audioClip: {fileID: 0}
  - dialogue: My name is Owen, I'm a detective investigating a string of missing
      persons cases.
    name: Owen Stone
    bust: {fileID: 21300000, guid: df9dfba447d21d74c9d483b02f3f8b20, type: 3}
    bustSide: 0
    audioClip: {fileID: 0}
  - dialogue: Right now I need to get the VR machine fixed, and I heard you're the
      right man for the job.
    name: Owen Stone
    bust: {fileID: 21300000, guid: df9dfba447d21d74c9d483b02f3f8b20, type: 3}
    bustSide: 0
    audioClip: {fileID: 0}
  - dialogue: I think I could take a look at it? With the manager's permission of
      course.
    name: Michael
    bust: {fileID: 0}
    bustSide: 1
    audioClip: {fileID: 0}
  - dialogue: He's the one who sent me.
    name: Owen Stone
    bust: {fileID: 21300000, guid: df9dfba447d21d74c9d483b02f3f8b20, type: 3}
    bustSide: 0
    audioClip: {fileID: 0}
  - dialogue: Oh... Well I guess I can try...
    name: Michael
    bust: {fileID: 0}
    bustSide: 1
    audioClip: {fileID: 0}
  - dialogue: How far in the game were you able to get?
    name: Michael
    bust: {fileID: 0}
    bustSide: 1
    audioClip: {fileID: 0}
  - dialogue: The first level I guess. It looked like a forest but weirder.
    name: Owen Stone
    bust: {fileID: 21300000, guid: df9dfba447d21d74c9d483b02f3f8b20, type: 3}
    bustSide: 0
    audioClip: {fileID: 0}
  - dialogue: It felt like I was a part of the game, and the monsters longed to put
      me 6 feet under.
    name: Owen Stone
    bust: {fileID: 21300000, guid: df9dfba447d21d74c9d483b02f3f8b20, type: 3}
    bustSide: 0
    audioClip: {fileID: 0}
  - dialogue: Yeah that's how that game is.
    name: Michael
    bust: {fileID: 0}
    bustSide: 1
    audioClip: {fileID: 0}
  - dialogue: But you'll need a different coin to reach level 2.
    name: Michael
    bust: {fileID: 0}
    bustSide: 1
    audioClip: {fileID: 0}
  - dialogue: I can give you mine if you do me a favor.
    name: Michael
    bust: {fileID: 0}
    bustSide: 1
    audioClip: {fileID: 0}
  - dialogue: I promised my sister a prize, but I'm no good at these games.
    name: Michael
    bust: {fileID: 0}
    bustSide: 1
    audioClip: {fileID: 0}
  - dialogue: Could you do it for me please?
    name: Michael
    bust: {fileID: 0}
    bustSide: 1
    audioClip: {fileID: 0}
  - dialogue: This game Mobile Axiom Fighter should give you enough.
    name: Michael
    bust: {fileID: 0}
    bustSide: 1
    audioClip: {fileID: 0}
  - dialogue: By the time you finish I should be done with the machine.
    name: Michael
    bust: {fileID: 0}
    bustSide: 1
    audioClip: {fileID: 0}
  - dialogue: I'll see what I can do.
    name: Owen Stone
    bust: {fileID: 21300000, guid: df9dfba447d21d74c9d483b02f3f8b20, type: 3}
    bustSide: 0
    audioClip: {fileID: 0}
